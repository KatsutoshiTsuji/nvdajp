complexSymbols:
# identifier	regexp
# Sentence endings.
. fim de sentença	(?<=[^\s.])\.(?=[\"'”’)\s]|$)
! fim de sentença	(?<=[^\s!])\!(?=[\"'”’)\s]|$)
? fim de sentença	(?<=[^\s?])\?(?=[\"'”’)\s]|$)
# Phrase endings.
; fim de período	(?<=[^\s;]);(?=\s|$)
: fim de período	(?<=[^\s:]):(?=\s|$)
# Others
ponto decimal	(?<=\d)\.(?=\d)
' em palavra	(?<=[^\W_])['’]

symbols:
# identifier	replacement[[	level][	preserve]][	# display name]

# Complex symbols
. fim de sentença	ponto final	all	always
! fim de sentença	exclamação	all	always
? fim de sentença	interrogação	all	always
; fim de período	ponto e vírgula	most	always
: fim de período	dois pontos	most	always
ponto decimal		none	always
' em palavra	apóstrofo	all	norep

# Whitespace
\0	em branco	char
\t	tab
\n	nova linha	char
\f	quebra de página	none
\r	retôrno	char
 	espaço	char
 	espaço	char	# no-break space


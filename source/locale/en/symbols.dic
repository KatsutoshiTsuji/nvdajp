complexSymbols:
# identifier	regexp
# Sentence endings.
. sentence ending	(?<=[^\s.])\.(?=[\"')\s]|$)
! sentence ending	(?<=[^\s!])\!(?=[\"')\s]|$)
? sentence ending	(?<=[^\s?])\?(?=[\"')\s]|$)
# Phrase endings.
; phrase ending	(?<=[^\s;]);(?=\s|$)
: phrase ending	(?<=[^\s:]):(?=\s|$)
# Others
decimal point	(?<=\d)\.(?=\d)

symbols:
# identifier	replacement	[level]	[preserve]	[display name]

# Complex symbols
. sentence ending	dot	-	a
! sentence ending	bang	-	a
? sentence ending	question	-	a
; phrase ending	semi	most	a
: phrase ending	colon	most	a
decimal point		none	a

# Whitespace
\0	blank	char
\t	tab
\n	line feed	char
\f	page break	none
\r	carriage return	char
 	space	char
 	space	char	-	no-break space


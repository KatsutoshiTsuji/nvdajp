msgstr " không có focus"

#: appModules\_default.py:217
msgid "move to focus"
msgstr "chuyển đến focus"

#: appModules\_default.py:219
msgid "Sets the navigator object to the current focus"
msgstr "chỉ đối tượng điều hướng đến focus hiện hành "

#: appModules\_default.py:231
msgid "No parents"
msgstr "Không có cấp cha"

#: appModules\_default.py:232
msgid "Sets the navigator object to the parent of the object it is currently on."
msgstr "Chuyển đối tượng điều hướng đến cấp cha của đối tượng đang đứng."

#: appModules\_default.py:244
msgid "No next"
msgstr "Không có kế tiếp"

#: appModules\_default.py:245
msgid "Sets the navigator object to the next object to the one it is currently on"
msgstr "Chuyển đối tượng điều hướng đến đối tượng kế đối tượng đang đứng."

#: appModules\_default.py:257
msgid "No previous"
msgstr "Không có về trước"

#: appModules\_default.py:258
msgid "Sets the navigator object to the previous object to the one it is currently on"
msgstr "Chuyển đối tượng điều hướng đến đối tượng đứng trước đối tượng đang đứng. "

#: appModules\_default.py:270
msgid "No children"
msgstr "Không có cấp con"

#: appModules\_default.py:271
msgid "Sets the navigator object to the first child object to the one it is currently on"
msgstr "Chuyển đối tượng điều hướng đến đối tượng con thứ nhất của đối tượng đang đứng."

#: appModules\_default.py:293
msgid "end of flow"
msgstr "Kết thúc cột báo"

#: appModules\_default.py:294
msgid "Sets the navigator object to the object this object flows to."
msgstr "Chuyển đối tượng điều hướng đến đối tượng mà đối tượng hiện hành đang đến."

#: appModules\_default.py:316
msgid "Beginning of flow"
msgstr "Bắt đầu cột báo"

#: appModules\_default.py:317
